  # Pods for App
   pod 'Parse'
   target 'AppTests' do
    inherit! :search_paths
    # Pods for testing
   pod 'Parse'
  end

  target 'AppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Parse'
  end

end
